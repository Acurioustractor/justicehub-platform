{
  "name": "youth-justice-service-finder",
  "version": "1.0.0",
  "description": "World-class service discovery system for Queensland youth justice services",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/api/server-simple.js",
    "start:fullstack": "npm run build:frontend && node src/api/server-fullstack.js",
    "start:supabase": "npm run build:frontend && node src/api/server-supabase.js",
    "import:supabase": "node scripts/import-to-supabase.js",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm ci && npm run build",
    "start:pipeline": "node src/pipeline-service.js",
    "dev": "nodemon src/api/server-simple.js",
    "dev:pipeline": "nodemon src/pipeline-service.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "eslint src --ext .js,.ts",
    "format": "prettier --write \"src/**/*.{js,ts,json}\"",
    "demo": "node final-demonstration.js",
    "demo:simple": "node scripts/demo-simple.js",
    "production:deploy": "bash scripts/production/deploy.sh",
    "production:backup": "bash scripts/production/backup.sh",
    "production:monitor": "bash scripts/production/monitor.sh",
    "health-check": "node scripts/health-check.js",
    "setup:db": "node scripts/setup-database-direct.js",
    "migrate": "node-pg-migrate",
    "seed": "node database/seeds/index.js",
    "scrape:production": "node scripts/run-all-scrapers-production.js",
    "scrape:demo": "node scripts/run-demo-scraper.js",
    "scrape:all": "node scripts/run-all-scrapers.js",
    "setup-elasticsearch": "node scripts/setup-elasticsearch.js",
    "temporal:setup": "./scripts/start-temporal.sh",
    "temporal:worker": "node src/temporal/worker.js",
    "temporal:scheduler": "node src/temporal/scheduler.js setup",
    "deploy:vercel": "cd frontend && vercel --prod",
    "deploy:railway": "railway up",
    "docker:build": "docker build -t youth-justice-finder .",
    "docker:up": "docker-compose -f docker-compose.production.yml up -d",
    "docker:down": "docker-compose -f docker-compose.production.yml down",
    "docker:logs": "docker-compose -f docker-compose.production.yml logs -f"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/static": "^7.0.4",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.1",
    "@supabase/supabase-js": "^2.52.0",
    "@temporalio/activity": "^1.12.1",
    "@temporalio/client": "^1.12.1",
    "@temporalio/worker": "^1.12.1",
    "@temporalio/workflow": "^1.12.1",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.1",
    "cheerio": "^1.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.3",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fastest-levenshtein": "^1.0.16",
    "fastify": "^4.25.2",
    "firecrawl": "^1.7.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "p-queue": "^8.0.1",
    "pg": "^8.16.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.0.0",
    "redis": "^4.7.1",
    "string-similarity": "^4.0.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^6.2.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
