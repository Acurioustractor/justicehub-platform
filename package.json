{
  "name": "justicehub-platform",
  "version": "1.0.0",
  "description": "Digital platform that transforms how grassroots organizations support young people",
  "main": "dist/server.js",
  "scripts": {
    "dev": "next dev",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:client": "next dev",
    "build": "npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "next build",
    "start": "next start",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx src/server/db/migrate.ts",
    "db:seed": "tsx src/server/db/seed.ts",
    "db:optimize": "tsx scripts/optimize-db.ts",
    "env:setup": "node scripts/env-setup.js",
    "env:validate": "node scripts/env-validate.js",
    "env:check": "npm run env:validate",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build"
  },
